{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "api/index.js"
    },
    {
      "src": "/upload",
      "dest": "api/index.js"
    },
    {
      "src": "/diagnosis.html",
      "dest": "public/diagnosis.html"
    },
    {
      "src": "/test.html", 
      "dest": "public/test.html"
    },
    {
      "src": "/style.css",
      "dest": "public/style.css"
    },
    {
      "src": "/script.js",
      "dest": "public/script.js"
    },
    {
      "src": "/favicon.ico",
      "dest": "public/favicon.ico"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
} 