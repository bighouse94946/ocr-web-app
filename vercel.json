{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "api/index.js"
    },
    {
      "src": "/upload",
      "dest": "api/index.js"
    },
    {
      "src": "/diagnosis.html",
      "dest": "api/index.js"
    },
    {
      "src": "/test.html", 
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ]
} 